<?xml version="1.0" encoding="UTF-8" ?>
<profile>
    <formats>
        <format>
            <name>avimux</name>
            <long_name>Audio Video Interleaved</long_name>
            <extensions>avi</extensions>
            <audio_codec>
                avenc_ac3, avenc_ac3_fixed, avenc_eac3, avenc_mp2,
                avenc_mp2fixed, avenc_wmav1, avenc_wmav2, @lamemp3enc, speexenc,
                vorbisenc
            </audio_codec>
            <video_codec>
                avenc_mpeg1video, avenc_mpeg2video, @avenc_mpeg4, avenc_wmv1,
                avenc_wmv2, theoraenc, vp8enc, vp9enc, x264enc
            </video_codec>
        </format>
        <format>
            <name>flvmux</name>
            <long_name>Flash Video</long_name>
            <extensions>flv</extensions>
            <audio_codec>@lamemp3enc, speexenc</audio_codec>
            <video_codec>avenc_mpeg4, @x264enc</video_codec>
        </format>
        <format>
            <name>matroskamux</name>
            <long_name>Matroska</long_name>
            <extensions>mkv</extensions>
            <audio_codec>
                avenc_ac3, avenc_ac3_fixed, avenc_eac3, avenc_mp2,
                avenc_mp2fixed, flacenc, lamemp3enc, opusenc, @vorbisenc
            </audio_codec>
            <video_codec>
                avenc_mpeg1video, avenc_mpeg2video, avenc_mpeg4, theoraenc,
                vp8enc, vp9enc, @x264enc, x265enc
            </video_codec>
        </format>
        <format>
            <name>mp4mux</name>
            <long_name>MPEG-4 Part 14</long_name>
            <extensions>mp4</extensions>
            <audio_codec>
                @avenc_ac3, avenc_ac3_fixed, avenc_eac3, avenc_mp2,
                avenc_mp2fixed, lamemp3enc, vorbisenc
            </audio_codec>
            <video_codec>
                avenc_mpeg1video, avenc_mpeg2video, avenc_mpeg4, @x264enc,
                x265enc
            </video_codec>
        </format>
        <format>
            <name>oggmux</name>
            <long_name>Ogg</long_name>
            <extensions>ogg, ogv</extensions>
            <audio_codec>vorbisenc</audio_codec>
            <video_codec>theoraenc</video_codec>
        </format>
        <format>
            <name>webmmux</name>
            <long_name>WebM</long_name>
            <extensions>webm</extensions>
            <audio_codec>opusenc</audio_codec>
            <video_codec>vp9enc</video_codec>
        </format>
    </formats>
    <codecs>
        <audio>
            <codec>
                <name>avenc_ac3</name>
                <long_name>ATSC A/52A (AC-3)</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>fltp</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>avenc_ac3_fixed</name>
                <long_name>ATSC A/52A (AC-3)</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>s16p</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>avenc_eac3</name>
                <long_name>ATSC A/52 E-AC-3</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>fltp</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>avenc_mp2</name>
                <long_name>MP2 (MPEG audio layer 2)</long_name>
                <supported_samplerates>
                    48000, 44100, 32000, 24000, 22050, 16000
                </supported_samplerates>
                <sample_fmts>s16</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>avenc_mp2fixed</name>
                <long_name>MP2 fixed point (MPEG audio layer 2)</long_name>
                <supported_samplerates>
                    48000, 44100, 32000, 24000, 22050, 16000
                </supported_samplerates>
                <sample_fmts>s16</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>avenc_wmav1</name>
                <long_name>Windows Media Audio 1</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>fltp</sample_fmts>
                <channel_layouts></channel_layouts>
            </codec>
            <codec>
                <name>avenc_wmav2</name>
                <long_name>Windows Media Audio 2</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>fltp</sample_fmts>
                <channel_layouts></channel_layouts>
            </codec>
            <codec>
                <name>flacenc</name>
                <long_name>FLAC (Free Lossless Audio Codec)</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>s32, s16</sample_fmts>
                <channel_layouts></channel_layouts>
            </codec>
            <codec>
                <name>lamemp3enc</name>
                <long_name>MP3 (MPEG audio layer 3)</long_name>
                <supported_samplerates>
                    48000, 44100, 32000, 24000, 22050, 16000, 12000, 11025,
                    8000
                </supported_samplerates>
                <sample_fmts>fltp, s32p, s16p</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>opusenc</name>
                <long_name>Opus</long_name>
                <supported_samplerates>48000, 24000, 16000, 12000, 8000</supported_samplerates>
                <sample_fmts>flt, s16</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>speexenc</name>
                <long_name>Speex</long_name>
                <supported_samplerates>32000, 16000, 8000</supported_samplerates>
                <sample_fmts>s16</sample_fmts>
                <channel_layouts>mono, stereo</channel_layouts>
            </codec>
            <codec>
                <name>vorbisenc</name>
                <long_name>Vorbis</long_name>
                <supported_samplerates></supported_samplerates>
                <sample_fmts>fltp</sample_fmts>
                <channel_layouts></channel_layouts>
            </codec>
        </audio>
        <video>
            <codec>
                <name>avenc_mpeg1video</name>
                <long_name>MPEG-1 video</long_name>
                <supported_framerates>
                    24000/1001, 24/1, 25/1, 30000/1001, 30/1, 50/1, 60000/1001,
                    60/1, 15/1, 5/1, 10/1, 12/1, 15/1
                </supported_framerates>
                <pix_fmts>yuv420p</pix_fmts>
            </codec>
            <codec>
                <name>avenc_mpeg2video</name>
                <long_name>MPEG-2 video</long_name>
                <supported_framerates>
                    1/1, 2/1, 3/1, 4/1, 5/1, 6/1, 8/1, 9/1, 10/1, 12/1, 15/1,
                    16/1, 18/1, 20/1, 24/1, 25/1, 30/1, 32/1, 36/1, 40/1, 45/1,
                    48/1, 50/1, 60/1, 72/1, 75/1, 80/1, 90/1, 96/1, 100/1,
                    120/1, 150/1, 180/1, 200/1, 240/1, 750/1001, 800/1001,
                    960/1001, 1000/1001, 1200/1001, 1250/1001, 1500/1001,
                    1600/1001, 1875/1001, 2000/1001, 2400/1001, 2500/1001,
                    3000/1001, 3750/1001, 4000/1001, 4800/1001, 5000/1001,
                    6000/1001, 7500/1001, 8000/1001, 10000/1001, 12000/1001,
                    15000/1001, 20000/1001, 24000/1001, 30000/1001, 60000/1001
                </supported_framerates>
                <pix_fmts>yuv420p, yuv422p</pix_fmts>
            </codec>
            <codec>
                <name>avenc_mpeg4</name>
                <long_name>MPEG-4 part 2</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>yuv420p</pix_fmts>
            </codec>
            <codec>
                <name>avenc_wmv1</name>
                <long_name>Windows Media Video 7</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>yuv420p</pix_fmts>
            </codec>
            <codec>
                <name>avenc_wmv2</name>
                <long_name>Windows Media Video 8</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>yuv420p</pix_fmts>
            </codec>
            <codec>
                <name>theoraenc</name>
                <long_name>Theora</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>yuv420p, yuv422p, yuv444p</pix_fmts>
            </codec>
            <codec>
                <name>vp8enc</name>
                <long_name>VP8</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>yuv420p, yuva420p</pix_fmts>
            </codec>
            <codec>
                <name>vp9enc</name>
                <long_name>VP9</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>yuv420p, yuv422p, yuv440p, yuv444p</pix_fmts>
            </codec>
            <codec>
                <name>x264enc</name>
                <long_name>H.264 / AVC / MPEG-4 AVC / MPEG-4 part 10</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>
                    yuv420p, yuvj420p, yuv422p, yuvj422p, yuv444p, yuvj444p,
                    nv12, nv16, nv21
                </pix_fmts>
            </codec>
            <codec>
                <name>x265enc</name>
                <long_name>H.265 / HEVC</long_name>
                <supported_framerates></supported_framerates>
                <pix_fmts>
                    yuv420p, yuv422p, yuv444p, yuv420p10le, yuv422p10le,
                    yuv444p10le
                </pix_fmts>
            </codec>
        </video>
    </codecs>
</profile>
